		"@types/node": "^22.15.3",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.3",
		"@vitejs/plugin-react-swc": "^3.9.0",
		"globals": "^16.0.0",
		"typescript": "~5.8.0",
		"vite": "^6.3.4",
		"wrangler": "^4.14.1"
	}
