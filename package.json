		"semver": "7.7.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/node": "24.5.2",
		"@types/react": "19.1.14",
		"@types/react-dom": "19.1.9",
		"@vitejs/plugin-react-swc": "4.1.0",
		"globals": "16.4.0",
		"typescript": "5.9.2",
		"vite": "7.1.7",
		"wrangler": "4.40.2"
	}
