{
	"name": "javadoc-page-template",
	"version": "0.0.0",
	"type": "module",
	"packageManager": "pnpm@10.10.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"check": "pnpm biome check ./",
